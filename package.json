{
  "name": "repo-rewind",
  "version": "1.0.0",
  "private": true,
  "description": "Repo Rewind",
  "author": "theshteves",
  "scripts": {
    "develop": "gatsby develop -p 4000 -- --https",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve -p 4000",
    "clean": "gatsby clean",
    "test": "cypress open",
    "lint": "eslint src cypress --ext .js,.jsx --ignore-pattern '**/examples/*'",
    "lint:fix": "npm run lint -- --fix",
    "refresh": "curl -X POST http://localhost:4000/__refresh"
  },
  "dependencies": {
    "gatsby": "^3.13.0",
    "gatsby-plugin-image": "^1.13.0",
    "gatsby-plugin-manifest": "^3.13.0",
    "gatsby-plugin-postcss": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-plugin-sitemap": "^4.9.0",
    "gatsby-source-filesystem": "^3.13.0",
    "gatsby-source-github": "^0.0.2",
    "gatsby-transformer-sharp": "^3.13.0",
    "postcss": "^8.3.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.3.4",
    "cypress": "^8.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-pages": "^3.2.3",
    "tailwindcss": "^2.2.14"
  }
}
